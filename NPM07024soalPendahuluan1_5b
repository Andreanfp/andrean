package npm07024soalpendahuluan1_5b;
public class NPM07024soalPendahuluan1_5b {
    public static void main(String[] args)  {
        int i=10;
        while (i>0)
        {
            int t=i%2;
            if ( t==0 )
            {
                System.out.print(i+" ");
            }
            i--;
        }
        System.out.println();
    }
}
