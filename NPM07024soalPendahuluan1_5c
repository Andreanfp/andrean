package npm07024soalpendahuluan1_5c;
public class NPM07024soalPendahuluan1_5c {
public static void main(String[] args) 
    {
        int i=10;
        do
        {
            int t=i%2;
            if ( t==0 )
            {
                System.out.print( i+" " );
            }
            i--;
        }
        while (i>0);
        System.out.println();
    }    
}
